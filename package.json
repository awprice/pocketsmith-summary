{
  "name": "pocketsmith-summary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-netlify": "rm -rf functions/* && netlify-lambda serve src-functions",
    "serve": "npm run serve-netlify & vue-cli-service serve",
    "build": "netlify-lambda build src-functions && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^3.9.97",
    "apollo-link": "^1.2.13",
    "apollo-server-lambda": "^2.14.2",
    "axios": "^0.21.1",
    "bufferutil": "^4.0.1",
    "cacache": "^11.3.3",
    "chart.js": "^2.9.3",
    "element-ui": "^1.4.13",
    "encoding": "^0.1.12",
    "eslint-config-airbnb": "^17.1.1",
    "format-currency": "^1.0.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.9.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-fquarter": "^1.0.0",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^4.13.1",
    "parse-link-header": "^2.0.0",
    "randomcolor": "^0.5.4",
    "sass-loader": "^7.3.1",
    "utf-8-validate": "^5.0.2",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.3",
    "vue-chartjs": "^3.5.0",
    "vue-cli-plugin-apollo": "^0.19.2",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-vue": "^5.2.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "eslint-config-airbnb-base"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "semi": [
        "error",
        "always"
      ],
      "import/no-unresolved": "ignore"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
